<?xml version="1.0" encoding="utf-8" ?>

<resources>
    <!--Replace placeholder ID with your tracking ID-->
    <string name="ga_trackingId">UA-44210834-1</string>

    <!--Enable automatic activity tracking-->
    <bool name="ga_autoActivityTracking">true</bool>

    <!--Todo change/remove once not debugging-->
    <string name="ga_logLevel">verbose</string>

    <!--Is in seconds, todo change to higher when not debugging-->
    <integer name="ga_dispatchPeriod">3</integer>

    <!-- Set session time out to 60 seconds -->
    <integer name="ga_sessionTimeout">60</integer>

    <!--Enables writing of debug information to the log-->
    <bool name="ga_debug">true</bool>

    <!--Enable automatic exception tracking-->
    <bool name="ga_reportUncaughtExceptions">true</bool>
</resources>